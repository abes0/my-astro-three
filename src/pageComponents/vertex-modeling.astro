---
import Layout from "@/layouts/Layout.astro";
---

<Layout>
<canvas id="canvas"></canvas>
</Layout>

<script>
  import Component from '@common/Component'
  import App from '@artworks/VertexModeling/App'
   new class extends Component {
    static selectorRoot = '#canvas'
    app: App | undefined
    init(): void {
      this.app = new App()
    }
  }
</script>
